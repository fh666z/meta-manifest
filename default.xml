<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote  name="poky"
           fetch="git://git.yoctoproject.org" />

  <remote  name="digilent"
           fetch="https://github.com/Digilent" />

  <remote  name="meta-oe"
           fetch="git://git.openembedded.org" />

  <remote  name="github"
           fetch="https://github.com" />

  <remote  name="myeng"
           fetch="https://github.com/fh666z" />

  <default revision="jethro"
           remote="poky"
           sync-j="4" />

  <!-- Default layers -->
  <project name="poky" remote="poky" path="poky"/>
  <project name="meta-openembedded" remote="meta-oe" path="meta-openembedded"/>
  <project name="meta-xilinx" remote="digilent" path="meta-xilinx"/>
  <project name="ZYBO" remote="digilent" path="ZYBO" revision="master"/>
  <project name="meta-manifest" remote="myeng" path="meta-manifest" revision="jethro"/>

</manifest>
