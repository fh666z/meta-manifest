<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote  name="poky"
           fetch="git://git.yoctoproject.org" />

  <remote  name="meta-oe"
           fetch="git://git.openembedded.org" />

  <remote  name="github"
           fetch="https://github.com" />

  <default revision="jethro"
           remote="poky"
           sync-j="4" />

  <!-- Default layers -->
  <project name="poky" remote="poky" path="poky"/>
  <project name="meta-openembedded" remote="meta-oe" path="meta-openembedded"/>
  <project name="Digilent/meta-xilinx" remote="github" path="meta-xilinx"/>
  <project name="Digilent/ZYBO" remote="github" path="ZYBO" revision="master"/>

</manifest>
